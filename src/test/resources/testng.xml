<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="QLDA Test Suite">
    <test verbose="2" preserve-order="true" name="Auth Tests">
        <classes>
            <class name="com.qlda.tests.RegisterTest"/>
            <class name="com.qlda.tests.LoginTest"/>
        </classes>
    </test>
    <test verbose="2" preserve-order="true" name="Project Tests">
        <classes>
            <class name="com.qlda.tests.ProjectListTest"/>
            <class name="com.qlda.tests.ProjectCreateTest"/>
            <class name="com.qlda.tests.ProjectEditTest"/>
            <class name="com.qlda.tests.ProjectDeleteTest"/>
        </classes>
    </test>
    <test verbose="2" preserve-order="true" name="Mission Tests">
        <classes>
            <class name="com.qlda.tests.MissionTest"/>
        </classes>
    </test>
    <test verbose="2" preserve-order="true" name="Upload File Tests">
        <classes>
            <class name="com.qlda.tests.UploadTest"/>
        </classes>
    </test>
    <test verbose="2" preserve-order="true" name="Chat Tests">
        <classes>
            <class name="com.qlda.tests.ChatTest"/>
            <class name="com.qlda.tests.ChatGroupTest"/>
            <class name="com.qlda.tests.ChatMemberTest"/>
        </classes>
    </test>
</suite>